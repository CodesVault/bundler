    "version": "0.0.1",
    "minimum-stability": "stable",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "CodesVault\\Bundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Keramot UL Islam",
            "email": "keramotul.islam@gmail.com"
        },
        {
            "name": "CodesVault Team",
            "email": "codesvault@gmail.com"
        }
    ],
    "require": {
        "symfony/finder": "^5.4",
        "vlucas/phpdotenv": "^5.5"
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    }
}
